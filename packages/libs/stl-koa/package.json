    "@types/koa-compose": "^3.2.8",
    "@types/koa-compress": "^4.0.6",
    "@types/koa__router": "^12.0.4",
    "@types/luxon": "^3.4.2",
    "form-data": "^4.0.1",
    "inlinable": "workspace:*",
    "openapi-types": "^12.1.3",
    "supertest": "^7.0.0"
  },
  "typedocMain": "src/index.ts"
